<!DOCTYPE html>
<html id="patternlab-html">
<head id="patternlab-head">
	<title id="title">Pattern Lab</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width" />
	<link rel="stylesheet" href="{{> assetRoot }}/css/styleguide.css" media="all" />
	<link rel="stylesheet" href="{{> assetRoot }}/css/prism.css" media="all" />
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="{{> assetRoot }}/js/jquery.js"><\/script>')</script>
</head>
<body id="patternlab-body">

	<!--Style Guide Header-->
	<header class="sg-header" role="banner">
		<a href="#sg-nav-container" class="sg-nav-toggle">Menu</a>
		<div class="sg-nav-container" id="sg-nav-container">
			{{> patternNav }}
			{{> ishControls }}
		</div>
	</header>
	<!--End Style Guide Header-->
	
	<!-- Iframe -->
	<div id="sg-vp-wrap">
		<div id="sg-cover"></div>
		<div id="sg-gen-container">
			<iframe id="sg-viewport" sandbox="allow-same-origin allow-scripts"></iframe>
			<div id="sg-rightpull-container">
				<div id="sg-rightpull"></div>
			</div>
		</div>
	</div>
	<!--end iFrame-->
	
	<script src="{{> assetRoot }}/js/prism.js"></script>
	<script src="{{> assetRoot }}/js/data-saver.js"></script>
	{{> patternPaths }}
	{{> viewAllPaths }}
	<script src="{{> assetRoot }}/js/url-handler.js"></script>
	{{> websockets }}
	<script src="{{> assetRoot }}/js/synclisteners.js"></script>
	<script src="{{> assetRoot }}/js/styleguide.js"></script>
	<!--<script src="{{> assetRoot }}/js/annotations-viewer.js "></script>-->
	<!--<script src="{{> assetRoot }}/js/code-viewer.js "></script>-->
</body>
</html>